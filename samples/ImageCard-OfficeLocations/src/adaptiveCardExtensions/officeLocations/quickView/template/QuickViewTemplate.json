{
  "schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "type": "AdaptiveCard",
  "version": "1.2",
  "body": [
    {
      "type": "ColumnSet",
      "columns": [
        {
          "type": "Column",
          "width": "20px",
          "items": [
            {
              "type": "Image",
              "color": "Light",
              "url": "data:image/png;base64,",
              "selectAction": {
                "type": "Action.Submit",
                "title": "Previous",
                "data": {
                  "id": "previous"
                }
              }
            }
          ],
          "verticalContentAlignment": "Center",
          "style": "default"
        },
        {
          "type": "Column",
          "width": "stretch",
          "items": [
            {
              "type": "TextBlock",
              "text": "${office.name} ${office.time}",
              "wrap": true,
              "size": "Large",
              "horizontalAlignment": "Center"
            }
          ]
        },
        {
          "type": "Column",
          "width": "20px",
          "items": [
            {
              "type": "Image",
              "url": "data:image/png;base64,",
              "selectAction": {
                "type": "Action.Submit",
                "title": "Next",
                "data": {
                  "id": "next"
                }
              }
            }
          ],
          "verticalContentAlignment": "Center"
        }
      ]
    },
    {
      "type": "Image",
      "url": "${office.locationMap.imageUrl}",
      "altText": "${office.locationMap.imageAlt}",
      "separator": true,
      "isVisible": "${showMaps}"
    },
    {
      "type": "TextBlock",
      "size": "Medium",
      "weight": "Bolder",
      "text": "${office.address}",
      "spacing": "Padding",
      "separator": true,
      "wrap": true,
      "horizontalAlignment": "Center"
    },
    {
      "type": "ColumnSet",
      "separator": true,
      "isVisible": "${showWeather}",
      "style": "accent",
      "columns": [
        {
          "type": "Column",
          "width": 20,
          "items": [
            {
              "type": "ColumnSet",
              "columns": [
                {
                  "type": "Column",
                  "width": "auto",
                  "items": [
                    {
                      "type": "Image",
                      "url": "${office.weather.icon}",
                      "size": "Small"
                    }
                  ]
                },
                {
                  "type": "Column",
                  "width": "auto",
                  "items": [
                    {
                      "type": "TextBlock",
                      "text": "${office.weather.temperature}",
                      "size": "ExtraLarge",
                      "spacing": "None",
                      "wrap": true
                    }
                  ]
                },
                {
                  "type": "Column",
                  "width": "stretch",
                  "items": [
                    {
                      "type": "TextBlock",
                      "text": "°C",
                      "weight": "Bolder",
                      "spacing": "Small",
                      "wrap": true
                    }
                  ]
                },
                {
                  "type": "Column",
                  "width": "auto",
                  "items": [
                    {
                      "type": "TextBlock",
                      "text": "High ${office.weather.high}°C",
                      "horizontalAlignment": "Left",
                      "wrap": true
                    },
                    {
                      "type": "TextBlock",
                      "text": "Low  ${office.weather.low}°C",
                      "horizontalAlignment": "Left",
                      "spacing": "None",
                      "wrap": true
                    },
                    {
                      "type": "TextBlock",
                      "text": "Wind ${office.weather.windSpeed}",
                      "horizontalAlignment": "Left",
                      "spacing": "None",
                      "wrap": true
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "ActionSet",
      "isVisible": "${office.locationMap.directionVisible}",
      "separator": true,
      "actions": [
        {
          "type": "Action.OpenUrl",
          "title": "Get directions",
          "style": "positive",
          "url": "${office.locationMap.directionUrl}",
          "tooltip": "Open directions in maps"
        }
      ]
    }
  ]
}