    "GenericTextImageAdaptiveCardExtensionStrings": "lib/adaptiveCardExtensions/genericTextImage/loc/{locale}.js",
    "GenericTextInputImageAdaptiveCardExtensionStrings": "lib/adaptiveCardExtensions/genericTextInputImage/loc/{locale}.js",
    "GenericTextInputButtonImageAdaptiveCardExtensionStrings": "lib/adaptiveCardExtensions/genericTextInputButtonImage/loc/{locale}.js",
