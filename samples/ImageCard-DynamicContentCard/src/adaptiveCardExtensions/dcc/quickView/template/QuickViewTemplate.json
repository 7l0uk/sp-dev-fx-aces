{
    "type": "AdaptiveCard",
    "body": [
        {
            "type": "Container",
            "items": [
                {
                    "type": "ColumnSet",
                    "columns": [
                        {
                            "type": "Column",
                            "width": "100px",
                            "items": [
                                {
                                    "type": "Image",
                                    "url": "data:image/png;base64,",
                                    "width": "80px"
                                }
                            ],
                            "bleed": true
                        },
                        {
                            "type": "Column",
                            "width": "stretch",
                            "items": [
                                {
                                    "type": "TextBlock",
                                    "text": "${quickViewTitle}",
                                    "wrap": true,
                                    "size": "Large",
                                    "weight": "Bolder",
                                    "horizontalAlignment": "Left"
                                }
                            ],
                            "verticalContentAlignment": "Center"
                        }
                    ]
                },
                {
                    "type": "TextBlock",
                    "size": "Medium",
                    "weight": "Bolder",
                    "text": "${quickViewSubtitle}"
                },
                {
                    "type": "TextBlock",
                    "text": "${quickViewText}",
                    "wrap": true
                }
            ],
            "style": "emphasis",
            "bleed": true
        },
        {
            "type": "Container",
            "$data": "${listItems}",
            "items": [
                {
                    "type": "ColumnSet",
                    "columns": [
                        {
                            "type": "Column",
                            "width": "60px",
                            "items": [
                                {
                                    "type": "Image",
                                    "url": "${Image}",
                                    "width": "70px"
                                }
                            ],
                            "verticalContentAlignment": "Center"
                        },
                        {
                            "type": "Column",
                            "width": "stretch",
                            "items": [
                                {
                                    "type": "TextBlock",
                                    "text": "${Title}",
                                    "wrap": true,
                                    "size": "Large",
                                    "horizontalAlignment": "Center",
                                    "weight": "Bolder"
                                },
                                {
                                    "type": "TextBlock",
                                    "text": "${Description}",
                                    "wrap": true,
                                    "size": "Default",
                                    "horizontalAlignment": "Center"
                                },
                                {
                                    "type": "ActionSet",
                                    "actions": [
                                        {
                                            "type": "Action.OpenUrl",
                                            "title": "Get Started",
                                            "url": "${Link}"
                                        }
                                    ],
                                    "spacing": "Small"
                                }
                            ],
                            "horizontalAlignment": "Center"
                        }
                    ],
                    "bleed": true
                }
            ],
            "height": "stretch",
            "spacing": "None",
            "bleed": true,
            "style": "default"
        }
    ],
    "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
    "version": "1.5"
}